{
    "name": "CBg",
    "version": "1.0",
    "description": "An extension that helps change the bg of a page",
    "permissions": ["declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "pophop.html",
        "default_icon": "icon.png"
    },
    "icons" : {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "manifest_version": 2
}